<h2>Lets Pick Some Food!</h2>


<%=form_for  @food do |f|%>
<%=f.label :name%>
<%= f.select :name, options_for_select(FoodItem.all.map {|p| [ p.name ]}.uniq,   "Select Food"),:prompt => "Select Food",:required => true %>
<%=f.label :quantity%>
<%=f.text_field :quantity%>

<%=f.submit 'Add Food!'%>
<%end%>



<hr>
