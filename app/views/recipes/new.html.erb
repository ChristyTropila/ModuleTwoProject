<h2> Let's Get Started! </h2>

<% if @errors %>
    <% @errors.each do |error|%>
   <p style= "color: red"> <%=error%> </p>
    <%end%>
<%end%>


<%=form_for  @recipe do |f|%>
<%=f.hidden_field :child_id, :value => session[:child_id]%>

<%=f.text_field :title%>
<%=f.submit 'Name Your Recipe!'%>
<%end%>
<br>
<%= link_to "Cancel", child_path(@current_child) %>


